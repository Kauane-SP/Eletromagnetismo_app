<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navegation_component"
    app:startDestination="@id/home">

    <activity
        android:id="@+id/slids"
        android:name="com.example.eletromagnetismo.view.Slids"
        android:label="activity_slids"
        tools:layout="@layout/activity_slids" />
    <fragment
        android:id="@+id/home"
        android:name="com.example.eletromagnetismo.fragments.Home"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_home_to_setUserFragmetn"
            app:destination="@id/setUserFragmetn" />
    </fragment>
    <fragment
        android:id="@+id/setUserFragmetn"
        android:name="com.example.eletromagnetismo.fragments.SetUserFragment"
        android:label="fragment_set_user_fragment"
        tools:layout="@layout/fragment_set_user" >
        <action
            android:id="@+id/action_setUserFragmetn_to_home"
            app:destination="@id/home" />
    </fragment>
</navigation>